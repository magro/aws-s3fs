AT_BANNER([MD5 Digest])

AT_SETUP([MD5DigestBuffer])
AT_CHECK([test-md5 MD5DigestBuffer ../../../README ], [], [stdout])
AT_CHECK([MD5SUM=`md5sum ../../../README | sed -n "s/^\([a-f0-9]\{32\}\).*$/\1/p"`  grep -e "$MD5SUM" stdout], [], [ignore])
AT_CLEANUP

AT_SETUP([MD5DigestStream])
AT_CHECK([test-md5 MD5DigestStream `which grep` ], [], [stdout])
AT_CHECK([GREP="`which grep`" MD5SUM=`md5sum $GREP | sed -n "s/^\([a-f0-9]\{32\}\).*$/\1/p"` grep -e "$MD5SUM" stdout], [], [ignore])
AT_CLEANUP

